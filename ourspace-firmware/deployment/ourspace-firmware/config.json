{
    "Hostname": "ourspace-firmware",
    "Update": {
        "HTTPPassword": "<redacted>"
    },
    "Environment": [
        "GOOS=linux",
        "GOARCH=amd64"
    ],
    "Packages": [
        "github.com/gokrazy/fbstatus",
        "github.com/gokrazy/hello",
        "github.com/gokrazy/serial-busybox",
        "github.com/gokrazy/breakglass",
        "github.com/gokrazy/mkfs",
        "github.com/cfhn/our-space/ourspace-firmware/cmd/ourspace-firmware"
    ],
    "PackageConfig": {
        "github.com/gokrazy/breakglass": {
            "ExtraFilePaths": {
                "/etc/breakglass.authorized_keys": "breakglass.authorized_keys"
            },
            "CommandLineFlags": [
                "-authorized_keys=/etc/breakglass.authorized_keys"
            ]
        },
        "github.com/gokrazy/gokrazy/cmd/randomd": {
            "ExtraFileContents": {
                "/etc/machine-id": "64df1bb2d07042038e2b839628a2835c\n"
            }
        }
    },
    "SerialConsole": "ttyS0,115200",
    "KernelPackage": "github.com/gokrazy/kernel.amd64",
    "FirmwarePackage": "github.com/gokrazy/kernel.amd64",
    "InternalCompatibilityFlags": {}
}
